{
  "name": "@spyrothon/uikit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./src/index.tsx",
  "source": "./src/index.tsx",
  "exports": {
    ".": {
      "import": "./dist/uikit.js",
      "require": "./dist/uikit.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "tsc": "tsc --watch",
    "types": "tsc --project tsconfig.json --declaration --noEmit false --emitDeclarationOnly --jsx react --module es6 --moduleResolution node --outDir dist/ --declarationMap",
    "build": "vite build && pnpm types",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "react": "16.8 - 18",
    "react-dom": "16.8 - 18"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "react-datepicker": "^4.8.0",
    "react-feather": "^2.0.10",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-datepicker": "^4.4.2",
    "@vitejs/plugin-react": "^2.1.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  }
}
